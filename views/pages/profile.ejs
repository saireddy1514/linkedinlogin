<!doctype html>
<html>

<head>
  <title>Linkedin</title>
  <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
  <link rel="stylesheet" type="text/css"
    href="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.97.5/css/materialize.min.css">
  <style>
    .card:hover {
      box-shadow: 0 10px 20px rgba(0, 0, 0, 0.19), 0 6px 6px rgba(0, 0, 0, 0.23);
      margin-bottom: 54px;
    }
    .navbar{
      background-color: purple;
    }
    .navbar-brand{
      font-size: 2rem;
      margin-left: 10px;
    }
    .nav-item{
      float: right;
      margin-right: 10px;
    }
    .card-action{
      color: purple;
    }
  </style>
</head>

<body>
  <nav class="navbar navbar-default">
  <div class="container-fluid">
    <div class="navbar-header">
      <a class="navbar-brand" href="#">HoloWorld</a>

      <a href="/logout" class="nav-item">Logout</a>
    </div>
  </div>
  <div class="section no-pad-bot" id="index-banner">
    <div class="container">
      <br><br>
      <div class="row center">
        <div class="col s12">
          <div class="card">
            <div class="card-action">

               <h5><b><%=displayName %></b></h5>
               <p><strong>Linkedin-Id</strong>: <%=id %></p>
               <p> <strong>FirstName:-</strong><%=givenName%> </p>
               <p><strong>LastName:-</strong><%=familyName%></p>
               <p><strong>Email:-</strong><%=email%></p>
               <p><strong>Local Ip:-</strong><%=localAddress%></p>
               <p><strong>Remote Ip:-</strong><%=remoteAddress%></p>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</body>

</html>
